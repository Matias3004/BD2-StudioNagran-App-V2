<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="150.0" prefWidth="240.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.studionagranapp.guicontrollers.userdashboard.ModifySessionController">
    <children>
        <BorderPane minHeight="-Infinity" minWidth="-Infinity" prefHeight="150.0" prefWidth="240.0">
            <center>
                <AnchorPane maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="150.0" prefWidth="240.0" style="-fx-background-color: #999999;" BorderPane.alignment="CENTER">
                    <children>
                        <Button layoutX="10.0" layoutY="104.0" mnemonicParsing="false" onAction="#changeDate" prefHeight="32.0" prefWidth="105.0" style="-fx-background-color: #EEEEEE;" text="Zmień date" textFill="#b25900">
                            <font>
                                <Font name="SF Pro Display Bold" size="14.0" />
                            </font>
                        </Button>
                        <Label layoutX="28.0" text="Podaj nową datę rozpoczęcia:">
                            <font>
                                <Font name="SF Pro Display Regular" size="13.0" />
                            </font>
                        </Label>
                  <DatePicker fx:id="newStartDateField" layoutX="27.0" layoutY="17.0" promptText="DD.MM.YYYY" />
                  <Label layoutX="27.0" layoutY="53.0" text="Podaj nową datę zakończenia:">
                     <font>
                        <Font name="SF Pro Display Regular" size="13.0" />
                     </font>
                  </Label>
                  <DatePicker fx:id="newEndDateField" layoutX="28.0" layoutY="70.0" promptText="DD.MM.YYYY" />
                  <Button fx:id="returnButton" layoutX="125.0" layoutY="104.0" mnemonicParsing="false" onAction="#close" prefHeight="32.0" prefWidth="105.0" style="-fx-background-color: #EEEEEE;" text="Powrót" textFill="#b25900">
                     <font>
                        <Font name="SF Pro Display Bold" size="14.0" />
                     </font>
                  </Button>
                    </children>
                </AnchorPane>
            </center>
        </BorderPane>
    </children>
</AnchorPane>
